1 PRINT SPC(35);"MULT-TABLE"
2 PRINT "THIS PROGRAM TESTS THE MULTIDIMENSIONAL ARRAY ALLOCATING AND INDEXING"
3 PRINT
4 OPTIONBASE 1
5 OPTIONDEBUG 1
6 OPTIONTRACE 1
9 I=DIM(2)
10 PRINT "TABLE WIDTH";
11 INPUT I(1)
20 PRINT "TABLE HEIGHT";
21 INPUT I(2)
30 T=DIM(I(2),I(1))
40 FOR Y=1 TO I(2)
50   FOR X=1 TO I(1)
60     T(Y,X)=X*Y
70   NEXT
80 NEXT
90 RESOLVE T
100 FOR Y=1 TO I(2)
110   FOR X=1 TO I(1)
120     PRINT T(Y,X);"  ";
130   NEXT
140   PRINT
150 NEXT
200 PRINT "ENDE"
